<launch>
  <node pkg="jbd_bms_status" exec="jbd_bms_status_node" name="jbd_bms_status_node" output="screen">
    <!-- param name="port" value="/dev/jbd_bms"/ -->
    <param name="port" value="/dev/serial/by-id/usb-1a86_USB_Single_Serial_575C024888-if00"/>
    <param name="frame_id" value="jbd_bms" />
    <param name="baudrate" value="9600" />
    <param name="looprate" value="1" />
    <param name="log_level" value="WARN" />
    <remap from="/jbd_bms_status/jbd_bms" to="/bms"/>
    <remap from="/jbd_bms_status/voltage" to="/battery/voltage"/>
    <remap from="/jbd_bms_status/current" to="/battery/current"/>
    <remap from="/jbd_bms_status/residual_capacity" to="/battery/residual_capacity"/>
    <remap from="/jbd_bms_status/design_capacity" to="/battery/design_capacity"/>
    <remap from="/jbd_bms_status/cycle_index" to="/battery/cycle_index"/>
    <remap from="/jbd_bms_status/date_production" to="/battery/date_production"/>
    <remap from="/jbd_bms_status/status_balance" to="/battery/status_balance"/>
    <remap from="/jbd_bms_status/status_protect" to="/battery/status_protect"/>
    <remap from="/jbd_bms_status/version" to="/battery/version"/>
    <remap from="/jbd_bms_status/rsoc" to="/battery/rsoc"/>
    <remap from="/jbd_bms_status/status_mos" to="/battery/status_mos"/>
    <remap from="/jbd_bms_status/cell_number" to="/battery/cell_number"/>
    <remap from="/jbd_bms_status/ntc_number" to="/battery/ntc_number"/>
    <remap from="/jbd_bms_status/cell_voltage" to="/battery/cell_voltage"/>
    <remap from="/jbd_bms_status/ntc_temperature" to="/battery/ntc_temperature"/>
    <remap from="/jbd_bms_status/error_id" to="/battery/error_id"/>
    <remap from="/jbd_bms_status/error_info" to="/battery/error_info"/>
  </node>
</launch>
